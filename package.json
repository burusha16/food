{
  "name": "food-party",
  "version": "0.0.1",
  "scripts": {
    "start": "ng serve --port=4400",
    "build:prerender": "npm run build:universal && npm run generate:prerender",
    "build:server": "ng run foodparty:server:production && npm run bundle-server-prerender",
    "build:server:serve": "npm run build:server && node prerender.js && node server.js",
    "build:universal": "ng build --prod --extractCss=true && ng run foodparty:server:production && npm run bundle-server-prerender",
    "bundle-server-prerender": "webpack --config webpack.config.js --progress --colors",
    "deploy": "heroku container:push web --app burusha-angular && heroku container:release web --app burusha-angular",
    "generate:prerender": "node prerender.js",
    "server": "node server.js",
    "ssr:debug": "ng build --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true --extractCss=true && ng run foodparty:server:production && webpack && node server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.0",
    "@angular/cdk": "^7.0.2",
    "@angular/common": "^7.2.0",
    "@angular/compiler": "^7.2.0",
    "@angular/core": "^7.2.0",
    "@angular/forms": "^7.2.0",
    "@angular/http": "^7.2.0",
    "@angular/material": "^7.0.2",
    "@angular/platform-browser": "^7.2.0",
    "@angular/platform-browser-dynamic": "^7.2.0",
    "@angular/platform-server": "^7.2.0",
    "@angular/router": "7.2.0",
    "@gorniv/ngx-transfer-http": "^2.2.9",
    "@nguniversal/common": "^7.0.2",
    "@nguniversal/express-engine": "^7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "@ngx-meta/core": "^6.0.0",
    "@ngx-translate/core": "^11.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/lodash": "^4.14.118",
    "@types/swiper": "^4.2.4",
    "core-js": "*",
    "domino": "^2.1.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "ngx-cookie": "^4.1.2",
    "ngx-swiper-wrapper": "^7.0.0",
    "rxjs": "^6.3.3",
    "rxjs-tslint": "^0.1.6",
    "swiper": "^4.4.2",
    "tslib": "^1.9.0",
    "webpack-cli": "^3.1.2",
    "zone.js": "~0.8.27"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.1",
    "@angular/cli": "~7.2.1",
    "@angular/compiler-cli": "^7.2.0",
    "@angular/language-service": "^7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.11.6",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "^3.2.2"
  }
}
